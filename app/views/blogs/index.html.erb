<p id="notice"><%= notice %></p>

<h1>Blogs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>
  <%= link_to '新規追加', new_blog_path, class: "btn btn-light" %>

  <ul>
    <% @blogs.each do |blog| %>
      <li>
        <%= blog.created_at %>
        <br>
        <%= link_to blog.title, blog %>
      </li>
    <% end %>
  </ul>
</table>

<br>

